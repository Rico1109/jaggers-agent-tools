{
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "name": "skill-discovery",
            "type": "command",
            "command": "$GEMINI_PROJECT_DIR/hooks/skill-discovery.py",
            "timeout": 5000,
            "description": "Automatically loads available skills from @skills/ into the agent context."
          }
        ]
      }
    ]
  },
  "hooksConfig": {
    "disabled": [
      "generated-hook-0",
      "generated-hook-0",
      "generated-hook-1",
      "generated-hook-0",
      "generated-hook-0",
      "generated-hook-0",
      "generated-hook-1",
      "skill-discovery"
    ]
  },
  "mcpServers": {
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant"
      ]
    },
    "context7": {
      "url": "https://mcp.context7.com/mcp",
      "type": "http",
      "headers": {}
    },
    "github-grep": {
      "url": "https://mcp.grep.app",
      "type": "http"
    },
    "deepwiki": {
      "url": "https://mcp.deepwiki.com/mcp",
      "type": "http"
    },
    "omni-search-engine": {
      "url": "http://127.0.0.1:8765/sse",
      "type": "sse"
    }
  }
}