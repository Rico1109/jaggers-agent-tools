{
  "mcpServers": {
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant"
      ]
    },
    "context7": {
      "url": "https://mcp.context7.com/mcp",
      "type": "http",
      "headers": {}
    },
    "github-grep": {
      "url": "https://mcp.grep.app",
      "type": "http"
    },
    "deepwiki": {
      "url": "https://mcp.deepwiki.com/mcp",
      "type": "http"
    }
  }
}